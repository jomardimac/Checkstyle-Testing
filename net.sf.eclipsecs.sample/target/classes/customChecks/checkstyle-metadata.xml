<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE checkstyle-metadata PUBLIC
"-//eclipse-cs//DTD Check Metadata 1.1//EN"
"http://eclipse-cs.sourceforge.net/dtds/checkstyle-metadata_1_1.dtd">
<checkstyle-metadata>
    <rule-group-metadata name="422Checks"  priority="1600">
        <description>%MyCustomChecks.desc</description>

        <rule-metadata name="%ExtremeContraction.name" internal-name="ExtremeContraction" parent="TreeWalker">
            <alternative-name internal-name="customChecks.ExtremeContractionCheck" />
            <description>%ExtremeContraction.desc</description>
            <property-metadata name="lowestAcceptableCount" datatype="Integer" default-value="2">
                <description>The lowest character count tolerable.</description>
            </property-metadata>
        </rule-metadata>
        <rule-metadata name="%MeaninglessTerms.name" internal-name="MeaninglessTerms" parent="TreeWalker">
            <alternative-name internal-name="customChecks.MeaninglessTermsCheck" />
            <description>%MeaninglessTerms.desc</description>
        </rule-metadata>
        <rule-metadata name="%UselessIdentifiers.name" internal-name="UselessIdentifiers" parent="TreeWalker">
            <alternative-name internal-name="customChecks.UselessIdentifiersCheck" />
            <description>%UselessIdentifiers.desc</description>
        </rule-metadata>
    </rule-group-metadata>
</checkstyle-metadata>
